            </form>

            <div class="subjects" id="subjects">
              {{{data.university.programs}}}
            </div>
          </section>
          {{/if}} {{!-- Selection criteria --}} {{#or data.university.BDielts data.university.MDielts data.university.BDtoefl data.university.MDtoefl
          data.university.BDpte data.university.MDpte data.university.gmat data.university.gre data.university.sat data.university.act
          data.university.gpa}}
          <section>
            <h3>Selection criteria</h3>
            {{#or data.university.BDielts data.university.MDielts data.university.BDtoefl data.university.MDtoefl data.university.BDpte
            data.university.MDpte}}
