                <div class="range text-center">
                  SAT
                  <strong>{{data.university.sat }}</strong>
                </div>
              </li>
              {{/if}} {{#if data.university.act}}
              <li class="col-md-3">
                <div class="range text-center">
                  ACT
                  <strong>{{data.university.act }}</strong>
                </div>
              </li>
              {{/if}} {{#if data.university.gpa}}
              <li class="col-md-3">
                <div class="range text-center">
                  GPA
                  <strong>{{data.university.gpa }}</strong>
                </div>
              </li>
              {{/if}}
            </ul>
            {{/or}}
          </section>
          {{/or}} {{!-- Academic life (year level, student clubs, female male ratio, academic calendar) --}} {{#or data.university.yearLevel
          data.university.studentClubs data.university.femaleMale data.university.academicCalendar}}
          <section>
            <h3>Academic Life</h3>
            <ul class="list-unstyled li-plain row">
              {{#if data.university.yearLevel}}
              <li class="col-md-4">
                <div class="value">{{data.university.yearLevel }}</div>
                <div class="title">
                  Year level
                </div>
              </li>
              {{/if}} {{#if data.university.studentClubs}}
              <li class="col-md-4">
                <div class="value">{{data.university.studentClubs }}</div>
                <div class="title">
                  Student clubs
                </div>
              </li>
              {{/if}} {{#if data.university.femaleMale}}
