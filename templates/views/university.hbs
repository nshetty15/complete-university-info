                    <span class="badge badge-1 float-right">{{data.university.country.currencyCode}} {{formatCurrency data.university.MDtuitionIn }}</span>
                    Domestic student tuition fee(avg.)
                  </li>
                  {{/if}} {{#if data.university.MDtuitionOut}}
                  <li class="list-group-item">
                    <span class="badge badge-1 float-right">{{data.university.country.currencyCode}} {{formatCurrency data.university.MDtuitionOut }}</span>
                    International student tuition fee(avg.)
                  </li>
                  {{/if}} {{#if data.university.MDprogramsCount}}
                  <li class="list-group-item">
                    <span class="badge badge-1 float-right">{{data.university.MDprogramsCount }}</span>
                    Total programs
                  </li>
                  {{/if}}
                </ul>
              </div>
              {{/or}}

          </section>
          {{/or}} {{!-- Subjects text-secondary --}}
          <section>
            <h3>Programs at {{data.university.name}}</h3>
            <div class="subjects">
              {{{data.university.programs}}}
            </div>
          </section>


          {{!-- Selection criteria --}} {{#or data.university.BDielts data.university.MDielts data.university.BDtoefl data.university.MDtoefl
          data.university.BDpte data.university.MDpte data.university.gmat data.university.gre data.university.sat data.university.act
          data.university.gpa}}
          <section>
            <h3>Selection criteria</h3>
            {{#or data.university.BDielts data.university.MDielts data.university.BDtoefl data.university.MDtoefl data.university.BDpte
