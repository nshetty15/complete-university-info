<section class="uni-banner bg-cui no-mb">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-md-4 col-lg-3">
        <div class="bg-white m-minus h-100 text-center">
          <div class="uni-logo">
            {{#if data.university.logo.version}}
            <img src="//res.cloudinary.com/uguide/image/upload/v{{data.university.logo.version}}/{{data.university.logo.public_id}}.jpg"
              alt="{{data.university.name}} logo" class="img-fluid" /> {{else}}
            <img src="/images/university_placeholder.png" class="img-fluid" /> {{/if}}
          </div>

          {{#if data.university.acronym}}
          <abbr title="{{data.university.name}}">
            <strong>{{data.university.acronym}}</strong>
          </abbr>
          {{/if}} {{#if data.university.founded}}
          <strong class="d-block">Established: {{data.university.founded}}</strong>
          {{/if}}
          <div class="text-left uni-contact">
            {{#if data.university.phone}}
            <div class="contact-row">
              <i class="fa fa-phone-square" aria-hidden="true"></i>
              {{#each data.university.phone}}
              <a href="tel:{{this}}">{{ this }} </a>
              {{/each}}
            </div>
            {{/if}} {{#if data.university.email}}
            <div class="contact-row">
              <i class="fa fa-envelope-square" aria-hidden="true"></i>
              {{#each data.university.email}}
              <a href="mailto:{{this}}?subject={{data.university.name}} &body=Hi, {{data.university.name}} - profile on www.universityinfo.com , you might like it {{baseUrl}}{{data.pathName}}">{{ this }} </a>
              {{/each}}</div>
            {{/if}}
          </div>
        </div>
      </div>
      <div class="col-xs-12 col-md-7 col-lg-8">
        <div class="padding-tb">
          <h1>
            {{data.university.name}}
          </h1>

          {{#if data.university.address}}
          <address>
            <i class="fa fa-map-marker" aria-hidden="true"></i> {{data.university.address}}
          </address>
          {{/if}}

          <div class="uni-brief"> {{{data.university.brief }}}</div>
        </div>
      </div>
    </div>
  </div>
</section>

<section class="uni-profile mb-0">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-md-8 col-lg-8 uni-primary">
        <div class="padding bg-white">
          {{!-- Badges --}}
