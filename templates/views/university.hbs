                    <strong>Bachelors degree</strong>
                  </li>
                  {{#if data.university.BDstudents}}
                  <li class="list-group-item">
                    <span class="badge badge-1 float-right">{{data.university.BDstudents }}</span>
                    Students
                  </li>
                  {{/if}} {{#if data.university.BDtuitionIn}}
                  <li class="list-group-item">
                    <span class="badge badge-1 float-right">{{data.university.country.currencyCode}} {{formatCurrency data.university.BDtuitionIn }}</span>
                    Domestic tuition fee(avg.)
                  </li>
                  {{/if}}{{#if data.university.BDtuitionOut}}
                  <li class="list-group-item">
                    <span class="badge badge-1 float-right">{{data.university.country.currencyCode}} {{formatCurrency data.university.BDtuitionOut }}</span>
                    International tuition fee(avg.)
                  </li>
                  {{/if}}{{#if data.university.BDprogramsCount}}
