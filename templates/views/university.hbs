                <div class="range text-center">
                  PTE (Masters)
                  <strong>{{data.university.MDpte }}</strong>
                </div>
              </li>
              {{/if}}
            </ul>
            {{/or}} {{#or data.university.gmat data.university.gre data.university.sat data.university.act data.university.gpa}}
