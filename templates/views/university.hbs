                </div>
              </li>
              {{/if}}
            </ul>
            {{/or}}
          </section>
          {{/or}} {{!-- Academic life (year level, student clubs, female male ratio, academic calendar) --}} {{#or data.university.yearLevel
          data.university.studentClubs data.university.femaleMale data.university.academicCalendar}}
