            <ul class="list-unstyled li-bg-grey row">
              {{#if data.university.BDielts}}
              <li class="col-md-3">
                <div class="range text-center">
                  IELTS (Bachelors)
                  <strong>{{data.university.BDielts }}</strong>
                </div>
              </li>
              {{/if}} {{#if data.university.MDielts}}
              <li class="col-md-3">
                <div class="range text-center">
                  IELTS (Masters)
