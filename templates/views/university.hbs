                <strong>Masters degree</strong>
              </li>
              {{#if data.university.MDstudents}}
              <li class="list-group-item">
                <span class="badge badge-1 float-right">{{data.university.MDstudents }}</span>
                Students
              </li>
              {{/if}} {{#if data.university.MDtuitionIn}}
              <li class="list-group-item">
                <span class="badge badge-1 float-right">{{data.university.country.currencyCode}} {{formatCurrency data.university.MDtuitionIn }}</span>
                Domestic student tuition fee(avg. per year*)
              </li>
              {{/if}} {{#if data.university.MDtuitionOut}}
              <li class="list-group-item">
                <span class="badge badge-1 float-right">{{data.university.country.currencyCode}} {{formatCurrency data.university.MDtuitionOut }}</span>
                International student tuition fee(avg. per year*)
              </li>
              {{/if}}
            </ul>
            {{/or}}
          </div>
          {{/or}}
        </div>
      </section>

    </section>

    <div class="row">
      {{!-- Left column --}}
      <div class="col-xs-12 col-md-8 col-lg-8 ">
        <div class="padding bg-white">


          {{!-- Subjects text-secondary --}} {{#if data.university.programs}}
