            <ul class="list-unstyled text-secondary text-center row">
              {{#if data.university.students}}
              <li class="col-md-4">
                <div class="value">{{formatCurrency data.university.students}}</div>
                <div class="title">
                  <strong>Total students</strong>
                </div>
              </li>
              {{/if}} {{#if data.university.outStudents}}
              <li class="col-md-4">
                <div class="value">{{data.university.outStudents }}</div>
                <div class="title">
                  <strong>International students</strong>
                </div>
              </li>
