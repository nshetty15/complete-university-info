                <div class="value">{{data.university.yearLevel }}</div>
                <div class="title">
                  Year level
                </div>
              </li>
              {{/if}} {{#if data.university.studentClubs}}
              <li class="col-md-4">
                <div class="value">{{data.university.studentClubs }}</div>
                <div class="title">
                  Student clubs
                </div>
              </li>
              {{/if}} {{#if data.university.femaleMale}}
