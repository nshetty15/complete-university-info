      {{!-- Right column --}}
      <div class="col-xs-12 col-md-4 col-lg-4">

        <aside class="uni-seconday bs m-minus text-secondary">

          {{!-- Motto, Locations, Application fee --}} {{#or data.university.motto data.university.locations data.university.serviceFee
          }}
          <section>
            {{#if data.university.motto}}
            <p title="{{data.university.motto}}">
              <strong>Motto:</strong> {{data.university.motto}}</p>
            {{/if}} {{#if data.university.locations}}
            <p>
              <strong>Other locations: </strong>
              {{data.university.locations }} </p>{{/if}} {{#if data.university.serviceFee}}
            <p>
              <strong>Service Fee: </strong>{{data.university.serviceFee }} </p>

            {{/if}}

          </section>
          {{/or}} {{!-- Accreditation and Recognition --}} {{#if data.university.accreditations}}
          <section>
            <h5>Accreditation and Recognition</h3>
              <ul class="list-group list-group-flush">
                {{#each data.university.accreditations}}
                <li class="list-group-item">
                  {{ this }}
                </li>
                {{/each}}
              </ul>
          </section>
          {{/if}} {{!-- Affiliations and Memberships --}} {{#if data.university.affiliations}}
          <section>
            <h5>Affiliations and Memberships</h3>
              <ul class="list-group list-group-flush">
                {{#each data.university.affiliations }}
                <li class="list-group-item">
                  {{ this }}
                </li>
                {{/each}}
              </ul>
          </section>
          {{/if}} {{!-- Facilities and Services --}} {{#or data.university.library data.university.housing data.university.sports data.university.distanceLearning
          }}
          <section>
            <h5>Facilities and Services</h3>
              <ul class="list-group">
                {{#if data.university.library}}
                <li class="list-group-item">
                  <span class="badge badge-1 float-right">{{data.university.library }}</span>Library</li>
                {{/if}} {{#if data.university.housing}}
                <li class="list-group-item">
                  <span class="badge badge-1 float-right">{{data.university.housing }}</span>Housing</li>
                {{/if}} {{#if data.university.sports}}
                <li class="list-group-item">
                  <span class="badge badge-1 float-right">{{data.university.sports }}</span>Sports facilities</li>
                {{/if}} {{#if data.university.distanceLearning}}
                <li class="list-group-item">
                  <span class="badge badge-1 float-right">{{data.university.distanceLearning }}</span>Distance learning</li>
                {{/if}}
              </ul>
          </section>
          {{/or}} {{!-- Notable Alumni --}} {{#if data.university.alumni }}
          <section class="h5">
            <h5>Notable Alumni</h3>
              {{#each data.university.alumni }}
              <span class="badge badge-info">{{ this }}</span>
              {{/each}}
          </section>
          {{/if}} {{!-- Admission (office,phone,rate)--}} {{#if data.university.admissionRate}}
          <section>
            {{!--
            <h5>Admission</h3> --}} {{!-- {{#if data.university.admissionOffice}}
              <address>{{data.university.admissionOffice }} </address>
              {{/if}} --}}
              <p>
                <strong>Admission rate: </strong>{{data.university.admissionRate }} </p>
          </section>
          {{/if}}

        </aside>

      </div>

    </div>

    <div class="pt-5">
      <div class="container">

        <nav aria-label="breadcrumb">
          <ol class="breadcrumb mb-0">
            <li class="breadcrumb-item">
              <a href="{{baseUrl}}/">
                <span class="sr-only">Home</span>
                <i class="fa fa-home" aria-hidden="true"></i>
              </a>
            </li>
            <li class="breadcrumb-item">
              <a href="{{baseUrl}}/universities/">Universities</a>
            </li>
            <li class="breadcrumb-item">
              <a href="{{baseUrl}}/universities/{{data.university.region.key}}/">{{data.university.region.name}}</a>
            </li>
            <li class="breadcrumb-item">
              <a href="{{baseUrl}}/universities/{{data.university.region.key}}/{{data.university.country.key}}/">{{data.university.country.name}}</a>
            </li>
            <li class="breadcrumb-item">
              <a href="{{baseUrl}}/universities/{{data.university.region.key}}/{{data.university.country.key}}/{{data.university.state.key}}/">{{data.university.state.name}}</a>
            </li>
            <li class="breadcrumb-item">
              <a href="{{baseUrl}}/universities/{{data.university.region.key}}/{{data.university.country.key}}/{{data.university.state.key}}/{{data.university.city.key}}/">{{data.university.city.name}}</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page">{{data.university.name}}</li>
          </ol>
        </nav>
      </div>
    </div>

  </div>

</section>