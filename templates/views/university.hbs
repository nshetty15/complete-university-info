              </a>
              {{/if}} {{#if data.university.vimeo}}
              <a href="{{data.university.vimeo }}" title="{{data.university.name}}, Vimeo profile" target="_blank" rel="noopener">
                <i class="fa fa-vimeo" aria-hidden="true"></i>
                <span class="sr-only">Vimeo</span>
              </a>
              {{/if}}
            </div>
          </section>
          </div>
        </div>

        {{!-- Right column --}}
        <div class="col-xs-12 col-md-4 col-lg-4">

          <aside class="uni-seconday bs m-minus text-secondary">

            {{!-- Motto, Locations, Application fee --}} {{#or data.university.appFee data.university.motto data.university.locations
            }}
            <section>
              {{#if data.university.motto}}
              <p title="{{data.university.motto}}">
                <strong>Motto:</strong> {{data.university.motto}}</p>
              {{/if}} {{#if data.university.locations}}
